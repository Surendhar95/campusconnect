I"J(function() {
  var countl;

  $(document).on('turbolinks:load', function() {
    var countl;
    return countl = $("#show_likes").text();
  });

  countl = parseInt(countl);

  $(document).on('click', '#likes', function() {
    var ans_id;
    ans_id = $(this).val();
    $.ajax('/comments/update', {
      type: 'PATCH',
      dataType: 'text',
      data: {
        ans_id: ans_id
      },
      success: function(data) {
        $("#likes").prop("disabled", true);
        return $("#show_likes").text(countl + 1);
      }
    });
    return false;
  });

  return;

}).call(this);
