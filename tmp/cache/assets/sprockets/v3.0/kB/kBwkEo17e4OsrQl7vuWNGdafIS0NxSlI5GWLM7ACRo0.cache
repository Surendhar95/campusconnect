I"	(function() {
  $(document).ready(function() {
    var ellipsestext, lesstext, moretext, showChar;
    showChar = 200;
    ellipsestext = '...';
    moretext = 'more';
    lesstext = 'less';
    $('.more').each(function() {
      var c, content, h, html;
      content = $(this).html();
      if (content.length > showChar) {
        c = content.substr(0, showChar);
        h = content.substr(showChar - 1, content.length - showChar);
        html = c + '<span class="moreellipses">' + ellipsestext + '&nbsp;</span><span class="morecontent"><span>' + h + '</span>&nbsp;&nbsp;<a href="" class="morelink">' + moretext + '</a></span>';
        $(this).html(html);
      }
    });
    $('.morelink').click(function() {
      if ($(this).hasClass('less')) {
        $(this).removeClass('less');
        $(this).html(moretext);
      } else {
        $(this).addClass('less');
        $(this).html(lesstext);
      }
      $(this).parent().prev().toggle();
      $(this).prev().toggle();
      return false;
    });
  });

}).call(this);
